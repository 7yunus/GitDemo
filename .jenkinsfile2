pipeline
{
agent any
tools { 
      maven 'MAVEN_HOME' 
      jdk 'JAVA_HOME' 
}
stages
{
stage('Build')
{
steps
{
sh "mvn clean"
}

}
stage('deploy')
{
steps{
echo 'deploying the code'
}

}
stage('test')
{
steps{
sh "mvn test"
}

}

stage('Unit Testing')
{
steps{
sh "mvn compile"
}

}
stage('Release')
{
when{
branch "origin/master"
}
steps{
echo 'releasing the project'
}

}



}

}
